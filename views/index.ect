<title>HotelExpress</title>
<body>
<script type="text/javascript" src="js/inputScript.js"></script>
<h1><%- @title %></h1>
<br/>
<br/>
<br/>

<div class="city">
<form action="http://localhost:5000/searchHotels" method="POST" enctype="mutlipart/form-data">
    <p>Destination: <input id="cities" type="text" name="cityName" autofocus /> </p>
    <p>Check-in Date: <input type="text" id="checkinDatePicker" name="checkinDate"></p>
    <p>Length: <input type="text" id="stepper" name="length"></p>
    <p>Min Rating: <select id="rating" name="minRating">
                      <option value="1">1</option>
                      <option value="2">2</option>
                      <option value="3">3</option>
                      <option value="4">4</option>
                      <option value="5">5</option>
        </select>
        </p>

    <input type="submit" />
</form>
  
</div>

